<!DOCTYPE suite SYSTEM "http://testng.org/testng-1.0.dtd" >

<suite name="Suite1" verbose="1" parallel="false">
    <test name="OCCheckoutTest">
        <classes>
            <class name="calculator.CalculatorEngineTest">
                <methods>
                    <include name="numericTest"/>
                    <exclude name="subtractionTest"/>
                    <include name="additionTest"/>
                    <include name="divisionTest"/>
                    <include name="divideZeroTest"/>
                    <include name="multiplicationTest"/>
                    <include name="doubleValueTest"/>
                </methods>
            </class>
        </classes>
    </test>
</suite>
